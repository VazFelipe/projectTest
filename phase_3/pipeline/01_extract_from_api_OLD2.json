								"value": "@pipeline().parameters.exclude_system_fields",
								"type": "Expression"
							},
							"limit": {
								"value": "@pipeline().parameters.limit",
								"type": "Expression"
							},
							"where": {
								"value": "@pipeline().parameters.where",
								"type": "Expression"
							},
							"start_date": {
								"value": "@pipeline().parameters.start_date",
								"type": "Expression"
							},
							"end_date": {
								"value": "@pipeline().parameters.end_date",
								"type": "Expression"
							}
						}
