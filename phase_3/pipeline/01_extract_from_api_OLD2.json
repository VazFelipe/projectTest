						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestResourcePoliceDepartment",
						"type": "DatasetReference",
						"parameters": {
