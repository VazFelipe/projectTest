{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-eastus-0001"
		},
		"RestApiSocrataService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestApiSocrataService'"
		},
		"dls_socrata_api_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dls_socrata_api'"
		},
		"RestApiSocrataService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat(concat(concat(concat(concat(concat(concat(concat(concat('https://data.sfgov.org/resource/wg3w-h783.json?$$exclude_system_fields=',linkedService().exclude_system_fields),'&$limit='),linkedService().limit),'&$where='),linkedService().where),'BETWEEN '),linkedService().start_date),' AND '), linkedService().end_date)}"
		},
		"RestApiSocrataService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "2osd2wzioj43v7iymmfnt74x3"
		},
		"dls_socrata_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsdeveastus0001.dfs.core.windows.net/"
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_exclude_system_fields": {
			"type": "string",
			"defaultValue": "False"
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_limit": {
			"type": "string",
			"defaultValue": "9999999999"
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_where": {
			"type": "string",
			"defaultValue": "report_datetime "
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_dataset": {
			"type": "string",
			"defaultValue": "police_department"
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_field_name": {
			"type": "string",
			"defaultValue": "report_datetime"
		},
		"everyday_11AM_properties_01_extract_from_api_parameters_response_format": {
			"type": "string",
			"defaultValue": ".json"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-eastus-0001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"RestApiSocrataService_password": {
						"value": "[parameters('RestApiSocrataService_password')]"
					},
					"dls_socrata_api_accountKey": {
						"value": "[parameters('dls_socrata_api_accountKey')]"
					},
					"RestApiSocrataService_properties_typeProperties_url": {
						"value": "[parameters('RestApiSocrataService_properties_typeProperties_url')]"
					},
					"RestApiSocrataService_properties_typeProperties_userName": {
						"value": "[parameters('RestApiSocrataService_properties_typeProperties_userName')]"
					},
					"dls_socrata_api_properties_typeProperties_url": {
						"value": "[parameters('dls_socrata_api_properties_typeProperties_url')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_exclude_system_fields": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_exclude_system_fields')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_limit": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_limit')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_where": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_where')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_dataset": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_dataset')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_field_name": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_field_name')]"
					},
					"everyday_11AM_properties_01_extract_from_api_parameters_response_format": {
						"value": "[parameters('everyday_11AM_properties_01_extract_from_api_parameters_response_format')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}